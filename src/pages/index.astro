---
import FormPreregistration from "../components/FormPreregistration.astro";
import Layout from "../layouts/Layout.astro";
import background from "../assets/background.svg";
---

<Layout>
  <img src={background.src} alt="" fetchpriority="high" class="fixed top-0 left-0 w-full h-full blur-3xl -z-10 grayscale-75" />
  <div
    class="container mx-auto h-full flex flex-col justify-center items-center"
  >
    <h1 class="text-center text-2xl font-bold">POC Form Register Wizard</h1>
    <FormPreregistration />
  </div>
</Layout>
